extends layout

block content
  h1= title
  - const image_names = ['french door.jpg', 'sliding door.jpg', 'fiber glass.jpg', 'storm-doors.jpeg', 'Monumental.jpg']

  - for (let i = 0; i < 3; i++)
    - let randomIndex = Math.floor(Math.random() * 5)
    
    img(
      src=`/images/${image_names[randomIndex]}`,
      alt=`Image ${randomIndex + 1}`,
      height=300,
      width=300
    )